package com.muxxu.fever.fevermap.components.scrollbar.skin {	import com.nurun.components.button.visitors.FrameVisitor;	import com.nurun.components.button.visitors.FrameVisitorOptions;	import com.nurun.components.scroll.scroller.scrollbar.ScrollbarSkin;	import flash.display.DisplayObject;	import flash.display.MovieClip;	/**	 * 	 * @author  Francois DURSUS for Nurun	 */	public class FeverScrollBarSkin extends ScrollbarSkin {								/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>FeverScrollBarSkin</code>.		 */		public function FeverScrollBarSkin(scrollerBt:MovieClip = null, scrollTrackBt:MovieClip = null) {			super(null, null, scrollerBt, null, scrollTrackBt);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/* ****** *		 * PUBLIC *		 * ****** */						/* ******* *		 * PRIVATE *		 * ******* */		/**		 * Initialize the class.<br>		 */		override protected function initialize(upBt:DisplayObject = null, downBt:DisplayObject = null, scrollerBt:DisplayObject = null, scrollerThumbMc:DisplayObject = null, scrollTrackBt:DisplayObject = null):void {			super.initialize(upBt, downBt, scrollerBt, scrollerThumbMc, scrollTrackBt);						var fVisitor:FrameVisitor;			var options:FrameVisitorOptions = new FrameVisitorOptions("out", "over", "down", "disabled", true, .25);			options.enableFrameFrom = "out";						if(_upBt.background is MovieClip) {				fVisitor = new FrameVisitor();				fVisitor.addTarget(_upBt.background as MovieClip);				_upBt.accept(fVisitor);			}						if(_downBt.background is MovieClip) {				fVisitor = new FrameVisitor();				fVisitor.addTarget(_downBt.background as MovieClip);				_downBt.accept(fVisitor);			}						if(_scrollerBt.background is MovieClip) {				fVisitor = new FrameVisitor();				fVisitor.addTarget(_scrollerBt.background as MovieClip, options);				_scrollerBt.accept(fVisitor);			}						if(_scrollTrackBt.background is MovieClip) {				fVisitor = new FrameVisitor();				fVisitor.addTarget(_scrollTrackBt.background as MovieClip, options);				_scrollTrackBt.accept(fVisitor);			}		}	}}