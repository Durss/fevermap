package com.muxxu.fever.fevermap.components.map {	import flash.events.Event;	import flash.geom.Rectangle;	/**	 * Event fired by the <code>MapEngine</code> component	 * 	 * @author  Francois	 */	public class MapEngineEvent extends Event {				public static const DATA_NEEDED:String	= "mapEngineDataNeeded";		public static const ZOOM_CHANGED:String	= "mapEngineZoomChanged";				private var _dataRect:Rectangle;										/* *********** *		 * CONSTRUCTOR *		 * *********** */		/**		 * Creates an instance of <code>MapEngineEvent</code>.		 */		public function MapEngineEvent(type:String, dataRect:Rectangle = null, bubbles:Boolean = false, cancelable:Boolean = false) {			_dataRect = dataRect;			super(type, bubbles, cancelable);		}						/* ***************** *		 * GETTERS / SETTERS *		 * ***************** */		/**		 * Gets the data rectangle to load.		 */		public function get dataRect():Rectangle {			return _dataRect;		}		/* ****** *		 * PUBLIC *		 * ****** */		override public function clone():Event {			return new MapEngineEvent(type, dataRect, bubbles, cancelable);		}						/* ******* *		 * PRIVATE *		 * ******* */	}}