package com.muxxu.fever.fevermap.components.map {		import flash.events.IEventDispatcher;
	/**	 * 	 * @author Francois	 */	public interface IZoomableMapItem extends IEventDispatcher {				/**		 * Updates the component with a specific zoom level.		 * 		 * @param zoom	if -1, keep the previously defined zoom value		 */		function setImageByZoomLevel(zoom:int):void;				/**		 * Gets the current zoom level of the component.		 */		function get zoomLevel():int;				/**		 * Defines if the item can be rendered or not.		 */		function set canRender(value:Boolean):void;			}}